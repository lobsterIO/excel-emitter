<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
    <!--import from uk.co.spudsoft.birt.emitters.excel-->
    <extension id="uk.co.spudsoft.birt.emitters2.excel" name="SpudSoft BIRT Emitters"
               point="org.eclipse.birt.report.engine.emitters">
        <emitter class="uk.co.spudsoft.birt.emitters.excel2.XlsxEmitter" format="xlsx2"
                 id="uk.co.spudsoft.birt.emitters.excel2.XlsxEmitter"
                 mimeType="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet"
                 pagination="page-break-pagination">
        </emitter>
        <emitter class="uk.co.spudsoft.birt.emitters.excel2.XlsEmitter" format="xls_spudsoft2"
                 id="uk.co.spudsoft.birt.emitters.excel2.XlsEmitter"
                 mimeType="application/vnd.ms-excel" pagination="page-break-pagination">
        </emitter>
    </extension>

</plugin>